    "@release-it/conventional-changelog": "^8.0.1",
    "countries-list": "^3.1.0",
    "eas-cli": "^8.0.0",
    "expo": "^51.0.8",
    "expo-checkbox": "~3.0.0",
    "expo-font": "~12.0.4",
    "expo-image-picker": "~15.0.5",
