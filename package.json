    "react-native-calendars": "^1.1304.1",
    "react-native-element-dropdown": "^2.12.0",
    "react-native-elements": "^3.4.3",
    "react-native-indicators": "^0.17.0",
    "react-native-modern-datepicker": "^1.0.0-beta.91",
    "react-native-otp-entry": "^1.6.1",
