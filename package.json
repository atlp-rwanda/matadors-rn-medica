{
  "name": "matadors-rn-medica",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "eas build -p android",
    "test": "jest --coverage",
    "test:watch": "jest --watchAll",
    "test:update": "jest --updateSnapshot",
    "ci-start": "expo build:web"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@commitlint/cli": "^19.3.0",
