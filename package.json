    "@babel/core": "^7.20.0",
    "@babel/plugin-transform-template-literals": "^7.24.1",
    "@eslint/js": "^9.1.1",
    "@types/react": "~18.2.45",
    "@types/react-native-indicators": "^0.16.6",
    "@types/react-native-modern-datepicker": "^1.0.5",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "globals": "^15.1.0",
    "jest": "^29.2.1",
    "jest-expo": "^51.0.2",
    "react-test-renderer": "18.2.0",
    "typescript": "~5.3.3",
    "typescript-eslint": "^7.8.0"
  },
  "release-it": {
    "git": {
      "commitMessage": "ch: release v${version}"
    },
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    }
  },
  "plugins": {
    "@release-it/conventional-changelog": {
      "infile": "CHANGELOG.md",
      "preset": {
        "name": "conventionalcommits",
        "types": [
          {
            "type": "ft",
            "section": "Features"
          },
          {
            "type": "bg",
            "section": "Bug fixes"
          },
          {
            "type": "rf",
            "section": "Refactors"
          }
        ]
      }
    }
  },
  "private": true
}
